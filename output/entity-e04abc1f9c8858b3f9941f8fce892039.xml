<md:EntityDescriptor entityID="http://federation.rijksmuseum.nl/adfs/services/trust"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:shibmd="urn:mace:shibboleth:metadata:1.0"
	xmlns:md="urn:oasis:names:tc:SAML:2.0:metadata"
	xmlns:mdui="urn:oasis:names:tc:SAML:metadata:ui">
	
	<md:IDPSSODescriptor
		protocolSupportEnumeration="urn:oasis:names:tc:SAML:2.0:protocol">
		<md:Extensions>
							<mdui:UIInfo>
				<mdui:DisplayName xml:lang="en"></mdui:DisplayName>
				<mdui:DisplayName xml:lang="nl"></mdui:DisplayName>
				<mdui:Description xml:lang="nl">Rijksmuseum Amsterdam</mdui:Description>
				<mdui:Description xml:lang="en">Rijksmuseum Amsterdam</mdui:Description>
								<mdui:Logo height="300" width="500">https://static.surfconext.nl/media/idp/rijksmuseum_logo.png</mdui:Logo>
								<mdui:Keywords xml:lang="nl">Rijksmuseum Amsterdam</mdui:Keywords>
				<mdui:Keywords xml:lang="en">Rijksmuseum Amsterdam</mdui:Keywords>
			</mdui:UIInfo>
		</md:Extensions>
		<md:KeyDescriptor xmlns:ds="http://www.w3.org/2000/09/xmldsig#"
			use="signing">
			<ds:KeyInfo>
				<ds:X509Data>
					<ds:X509Certificate></ds:X509Certificate>
				</ds:X509Data>
			</ds:KeyInfo>
		</md:KeyDescriptor>
		<md:NameIDFormat>urn:oasis:names:tc:SAML:2.0:nameid-format:persistent
			</md:NameIDFormat>
		<md:NameIDFormat>urn:oasis:names:tc:SAML:2.0:nameid-format:transient
			</md:NameIDFormat>
		<md:NameIDFormat>urn:oasis:names:tc:SAML:1.1:nameid-format:unspecified
			</md:NameIDFormat>
		<md:NameIDFormat>urn:oasis:names:tc:SAML:2.0:nameid-format:unspecified
			</md:NameIDFormat>
		<md:SingleSignOnService
			Binding="urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect"
			Location="https://stepup.surfconext.nl/authentication/idp/single-sign-on/key:default/e04abc1f9c8858b3f9941f8fce892039" />
	</md:IDPSSODescriptor>
	<md:Organization>
		<md:OrganizationName xml:lang="en">Rijksmuseum Amsterdam</md:OrganizationName>
		<md:OrganizationDisplayName xml:lang="en"></md:OrganizationDisplayName>
		<md:OrganizationURL xml:lang="en">http://www.rijksmuseum.nl/en</md:OrganizationURL>
	</md:Organization>
				
