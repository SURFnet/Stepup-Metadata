	</md:ContactPerson>
						
								<md:ContactPerson contactType="support">
		<md:GivenName>Paul van Kooten</md:GivenName>
		<md:EmailAddress>systeembeheer@rijksmuseum.nl</md:EmailAddress>
	</md:ContactPerson>
						
				<md:ContactPerson contactType="administrative">
		<md:GivenName>Rob Hendriks</md:GivenName>
		<md:EmailAddress>r.hendriks@rijksmuseum.nl</md:EmailAddress>
	</md:ContactPerson>
